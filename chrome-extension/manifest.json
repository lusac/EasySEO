{
  "manifest_version": 2,
  "name": "easySEO",
  "description": "Hackday",
  "version": "0.1",
  "background": {
    "scripts": ["context-menus.js"]
  },
   "content_scripts": [ {
      "js": ["content.js"],
      "css": ["content.css"],
      "matches": [ "<all_urls>" ]
   } ],
  "permissions": [
    "activeTab",
    "contextMenus"
  ]
}
